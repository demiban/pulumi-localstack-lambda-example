{
  "name": "pulumi-localstack",
  "version": "1.0.0",
  "description": "Example project for deploying aws serverless lambda to localstack",
  "main": "index.js",
  "scripts": {
    "build": "npm run build:getName && npm run build:getAddress",
    "build:getName": "ncc build functions/getName.js -m -e aws-sdk -o .build/getName",
    "build:getAddress": "ncc build functions/getAddress.js -m -e aws-sdk -o .build/getAddress"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/demiban/pulumi-localstack-lambda-example.git"
  },
  "keywords": [
    "pulumi",
    "localstack",
    "nodejs",
    "aws",
    "lambda",
    "test"
  ],
  "author": "Demiban Diaz Torres",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/demiban/pulumi-localstack-lambda-example/issues"
  },
  "homepage": "https://github.com/demiban/pulumi-localstack-lambda-example#readme",
  "dependencies": {
    "dotenv": "^6.2.0",
    "mime": "^2.4.0"
  },
  "devDependencies": {
    "@zeit/ncc": "^0.22.3",
    "eslint": "6.1.0",
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "prettier": "^1.18.2"
  }
}
